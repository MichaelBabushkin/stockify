{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["components/*"],
      "@/lib/*": ["lib/*"],
      "~fonts/*": ["public/fonts/*"],
      "~fonts": ["public/fonts"],
      "@images/*": ["public/static/images/*"],
      "@images": ["public/static/images"],
      "~sections": ["src/sections"],
      "~sections/*": ["src/sections/*"],
      "~styled/*": ["src/styles/*"],
      "~styled": ["src/styles"],
      "~scss/*": ["src/styles/scss/*"],
      "~scss": ["src/styles/scss"],
      "~data/*": ["src/data/*"],
      "~data": ["src/data"],
      "@/*": ["node_modules/*"],
      "@": ["node_modules"],
  }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "helpers/fileReadWrite.js", "pages/api/db/logo/getLogos.js", "pages/api/sp500stocks.js", "pages/index.js", "pages/api/auth/[...nextauth].ts", "pages/register/index.jsx", "pages/api/register/index.ts", "pages/login/index.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
